{
	"useTabs": true,
	"printWidth": 120,
	"tabWidth": 2,
	"semi": true,
	"singleQuote": false,
	"bracketSpacing": true,
	"bracketSameLine": false,
	"arrowParens": "avoid",
	"importOrder": ["(.+)/pages/(.+)", "(.+)/components/(.*)$", "(.+)/hooks/(.*)$", "(.+)/stores/(.*)$", "^[./]"],
	"importOrderSeparation": true,
	"importOrderSortSpecifiers": true
}
